$(document).on('click','.monogram_check input[type="checkbox"]', function () {
    var proid = $(this).parents('.cart-item').data('proid');
    var item = $(this).parents('.cart-item').data('item_id');
    var color = $(this).parents('.cart-item').find('.cart-item_color').attr('data-clr');
    var sku_product = $(this).parents('.cart-item').data('sku');
    var url = "https://oxaleather.com/search?view=monogram_view&q=id:"+proid+"&type=product&variant="+item
    $.ajax({
        type: "GET",
        url: url,
        success: function (msg) {
            var parser = new DOMParser();
            doc = parser.parseFromString(msg, "text/html");
            var remainder = doc.querySelector('[varid="'+item+'"]').innerHTML;
            var monoimgg = doc.querySelector('img.hideimagesvar[data-color="'+color+'"][alt*="monogram"]').getAttribute("src");
            $('.custommodal .modal-content.mono-radius').html(remainder);
            var mono_top = $('.custommodal .main-img-monogram_pop').data("top");
            var mono_left = $('.custommodal .main-img-monogram_pop').data("left");
            var mono_size = $('.custommodal .main-img-monogram_pop').data("size");
            $('.custommodal .main-img-monogram img').attr('src',monoimgg)
            $('.custommodal .add-to-cart button').attr('sku',sku_product)

            window.img_width = window.img_height = "";
            window.pop_stage = new Konva.Stage({
              container: '.main-img-monogram_pop',
              width: 400,
              height: 400
            });
            
            var layer = new Konva.Layer();
            pop_stage.add(layer);
            
            var imageObj = new Image();
            imageObj.onload = function () {
            window.img_width = imageObj.width; window.img_height = imageObj.height;
            var yoda = new Konva.Image({
              id: "mono_image",
              x: 0,
              y: 0,
              image: imageObj,
              width: 400,
              height: 400,
            });

            layer.add(yoda);
            var tt = $("#pop_name").val();
            var ff = $("span.right-click.righticon").parent().data("font");
            var simpleText = new Konva.Text({
              id: "mono_text",
              x: mono_left,
              y: mono_top,
              width:200,
              text: tt,
              fontSize: mono_size,
              fontFamily: ff,
              fontStyle: 'bold',
              fill: 'black',
              verticalAlign: 'middle',
              align: 'center',
              wrap: "word"
            });

          layer.add(simpleText);
        };
        imageObj.crossOrigin = 'Anonymous';
        imageObj.src = monoimgg;
            $('.custommodal#exmpModalToggleone').addClass('show');
            $('.custommodal#exmpModalToggleone').show();
            $(".custommodal .fonts-select .times").find(".right-click").addClass('righticon');
          $(document).on('input', '.custommodal .name-initial input', function(){
              var tx = window.pop_stage.findOne("#mono_text");
              tx.text($(this).val());
          })
        }
    });
});
