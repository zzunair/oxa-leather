<section id="MainProduct-{{ section.id }}" class="main--products page-width section-{{ section.id }}-padding" data-section="{{ section.id }}">
  {{ 'section-main-product.css' | asset_url | stylesheet_tag }}
  {{ 'component-accordion.css' | asset_url | stylesheet_tag }}
  {{ 'component-price.css' | asset_url | stylesheet_tag }}
  {{ 'component-rte.css' | asset_url | stylesheet_tag }}
  {{ 'component-slider.css' | asset_url | stylesheet_tag }}
  {{ 'component-rating.css' | asset_url | stylesheet_tag }}
  {{ 'component-loading-overlay.css' | asset_url | stylesheet_tag }}
  {{ 'component-deferred-media.css' | asset_url | stylesheet_tag }}

  {%- style -%}
    .section-{{ section.id }}-padding {
      padding-top: {{ section.settings.padding_top | times: 0.75 | round: 0 }}px;
      padding-bottom: {{ section.settings.padding_bottom | times: 0.75 | round: 0 }}px;
    }

    @media screen and (min-width: 750px) {
      .section-{{ section.id }}-padding {
        padding-top: {{ section.settings.padding_top }}px;
        padding-bottom: {{ section.settings.padding_bottom }}px;
      }
    }
  {%- endstyle -%}
   <div class="rightside">  
    <div class="contectdraw">
      <div>
      <img class="cartdr" src="">
      </div>
      <div class="addedcart">
      <p> Added to cart.</p>
      <span class="itemname"></span>
      </div>
      <span class="cross"><img src="https://cdn.shopify.com/s/files/1/0507/0418/7590/files/Vector_3.png?v=1653896313"></span>
    </div>
  </div>
  
  <div class="removedcart">
  
    <div class="removeddraw">
      <div>
      <img class="removeddr" src="">
      </div>
      <div class="remove-to-cart">
      <p> Removed from cart.</p>
      <span class="remveitemname"></span>
      </div>
      <span class="removecross"><img src="https://cdn.shopify.com/s/files/1/0507/0418/7590/files/Vector_3.png?v=1653896313"></span>
    </div>
  </div>
  
  <script type="application/json" id="product-collections" >[ {% for coll in product.collections %}"{{ coll.url }}" {% unless forloop.last %},{% endunless %} {% endfor %}]</script>

  
  {% capture monogrampro %}
  {% if product.tags contains 'iphone' %}
     {% assign coll = collections['iphone'].products %}
  {% elsif product.tags contains 'watch' %}
      {% assign coll = collections['watch'].products %}
  {% elsif product.tags contains 'airpods' %}
      {% assign coll = collections['airpods'].products %}
  {% endif %}

  <div class="accordion custom">
    {% for block in section.blocks %}
      {% if block.type=='accordion' %}
        <div class="content-container">
           <span class="que_icon">{{ block.settings.que_icon }}</span>
           <div class="space_eq"> <div class="question"><span class="quee">{{ block.settings.que }}</span></div>
            <div class="answer">{{ block.settings.ans }}</div></div>
        </div>
        {% endif %}
    {% endfor %}

    <div class="content_video">
        {% assign collection_gallery2 = product.metafields.custom.video_cart.value %}
         {{ collection_gallery2 | video_tag: image_size: '327x200', controls: true, loop: "true", autoplay: "autoplay", width: 327, height: 200 }} 
    </div> 
   </div>

    
   {% comment %}
  <div class="freqntly-boght-with"><div>FREQUENTLY BOUGHT WITH:</div></div>
  {% assign add_ons = product.metafields.my_fields.add_on %}
  
 {% if add_ons != blank %}
  {% assign add_ons = product.metafields.my_fields.add_on | split :"," %}
  <div class="monos" >                   
     {% for product_id in add_ons %}
    {% assign product_id =  product_id | strip %}
    {% assign product_v2 = all_products[product_id] %}
    {% if product_v2.available %}
    <div class="products-extra">      
       {% assign featured_image = product_v2.selected_or_first_available_variant.featured_image | default: product_v2.featured_image %}
      <div class="imges"><img src="{{ featured_image | img_url:'56x56' }}"></div>
      <div class="title-price">
        <div class="add_on_tilte aa">{{ product_v2.title }}<span class="pipe">|</span><span class="compare_at_price">{{ product_v2.compare_at_price | money | remove: '.00' }}</span><span>{{ product_v2.price | money | remove: '.00' }}</span></div>                 
      </div>
      {% assign exist_p = false %}
      {% for itm in cart.items %}
      	{% if itm.id == product_v2.selected_or_first_available_variant.id %}
      		{% assign exist_p = true %}
      	{% endif %}
      {% endfor %}
      
      <div class="bttn" data-toggle="modal" data-target="#myModal2" style="{% if exist_p %}display:none{% endif %}">
      	<a data-id="{{ product_v2.selected_or_first_available_variant.id }}">Add</a>
        <div class="loading-overlay__spinner hidden">
           <svg aria-hidden="true" focusable="false" role="presentation" class="spinner" viewBox="0 0 66 66" xmlns="http://www.w3.org/2000/svg">
              <circle class="path" fill="none" stroke-width="6" cx="33" cy="33" r="30" style="stroke: #fff;"></circle>
           </svg>
        </div>
      </div>
      <div class="click-cross">
      <span class="click-icon {% if exist_p %}click-icon-show{% endif %}">
       {% render 'click-icon' %}
      </span>
      <span class="cross-addon {% if exist_p %}cross-mobile{% endif %}">
        <img src="https://cdn.shopify.com/s/files/1/0507/0418/7590/files/Vector_3.png?v=1653896313">
      </span>
      </div>      
    </div>
    {% endif %}
    {% endfor %}
  </div>
  {% endif %}
 {% endcomment %}
  {% endcapture %}
  
  {% capture freeshiping %}
  <div class="free-shipp">
  <div class="free-ship-text">{{ section.settings.freeship }}</div>
  </div>
  {% endcapture %}
  
  {% capture shortdesc %}
    <p class="short-desc-pro">{{ product.metafields.my_fields.short_description }}</p>
  {% endcapture %}
  <script src="{{ 'product-form.js' | asset_url }}" defer="defer"></script>

  {%- assign first_3d_model = product.media | where: "media_type", "model" | first -%}
  {%- if first_3d_model -%}
    {{ 'component-product-model.css' | asset_url | stylesheet_tag }}
    <link id="ModelViewerStyle" rel="stylesheet" href="https://cdn.shopify.com/shopifycloud/model-viewer-ui/assets/v1.0/model-viewer-ui.css" media="print" onload="this.media='all'">
    <link id="ModelViewerOverride" rel="stylesheet" href="{{ 'component-model-viewer-ui.css' | asset_url }}" media="print" onload="this.media='all'">
  {%- endif -%}

   {% if collection.previous_product or collection.next_product or section.settings.product_breadcrumb %}
      <div class="clearfix breadcrumb-collection">
          {% render 'new-breadcrumb' %}
        {% if collection.previous_product or collection.next_product %}
          {% if section.settings.product_narow %}
            <div class="nav_arrows breadcrumb_text">
              {% if collection.previous_product %}
                <a href="{{ collection.previous_product }}" title="{{ 'products.general.previous_product_html' | t }}" class="breadcrumb_link"><span class="icon-left-arrow"> {{ 'products.general.previous_product_html' | t }}</span></a>
              {% endif %}
              {% if collection.previous_product and collection.next_product %}
              <span class="breadcrumb-divider"> {% render 'next-arrow' %} </span>
              {% endif %}

              {% if collection.next_product %}
                <a href="{{collection.next_product}}" title="{{ 'products.general.next_product_html' | t }}" class="breadcrumb_link">{{ 'products.general.next_product_html' | t }} <span class="icon-right-arrow"></span></a>
              {% endif %}
            </div>
          {% endif %} 
         
        {% endif %}

        {% if section.settings.product_breadcrumb %}
          <div class="breadcrumb_text" itemscope itemtype="http://schema.org/BreadcrumbList" style="display:none;">
            <span itemprop="itemListElement" itemscope itemtype="http://schema.org/ListItem">
              <a href="{{ shop.url }}" title="{{ shop.name | escape }}" itemprop="item" class="breadcrumb_link">
                <span itemprop="name">{{ 'general.breadcrumbs.home' | t }}</span>
              </a>
              <meta itemprop="position" content="1" />
            </span>
            <span class="breadcrumb-divider"> {% render 'next-arrow' %} </span>
            <span itemprop="itemListElement" itemscope itemtype="http://schema.org/ListItem">
              {% if collection %}
                <a href="{{ collection.url }}" title="{{ collection.title | escape }}" itemprop="item" class="breadcrumb_link">
                  <span itemprop="name">{{ collection.title }}</span>
                  <meta itemprop="position" content="2" />
                  </a>
              {% else %}
                <a href="/collections/all" title="{{ collections.all.title }}" itemprop="item" class="breadcrumb_link">
                  <span itemprop="name">{{ collections.all.title }}</span>
                  <meta itemprop="position" content="2" />
                  </a>
              {% endif %}
            </span>
            <span class="breadcrumb-divider"> {% render 'next-arrow' %} </span>
            <span itemprop="itemListElement" itemscope itemtype="http://schema.org/ListItem">
              <a itemprop="item" class="breadcrumb_link">
                <span itemprop="name">Cases</span>
                <meta itemprop="position" content="3" />
              </a>
            </span>
            <span class="breadcrumb-divider"> {% render 'next-arrow' %} </span>
            <span itemprop="itemListElement" itemscope itemtype="http://schema.org/ListItem">
              <a itemprop="item" class="breadcrumb_link">
                <span itemprop="name">{% if product.tags contains 'iphone' %}Iphone{% elsif product.tags contains 'watch' %}watch{% elsif product.tags contains 'airpods' %}airpods{% elsif product.tags contains 'ipad' %}Ipad{% endif %}</span>
                <meta itemprop="position" content="3" />
              </a>
            </span>
            <span class="breadcrumb-divider"> {% render 'next-arrow' %} </span>
            <span itemprop="itemListElement" itemscope itemtype="http://schema.org/ListItem">
              <a itemprop="item" class="breadcrumb_link last-child">
                <span itemprop="name">{{ product.title }}</span>
                <meta itemprop="position" content="3" />
              </a>
            </span>
          </div>
        {% endif %}
      
       
      </div>
    {% endif %}
  
 
  
